/* pages/applicant/applicant.wxss */
.container{
  padding-top: 0;
}

.item{
    width: 100%;
    height: 150rpx;
    position: relative; 
    display: flex;
    margin-top: 0rpx;
}

.name{
    margin: 50rpx 10rpx 50rpx 10rpx;
    font-size: 50rpx;
    overflow: hidden;
    flex: 1;
    padding-left: 300rpx;
    color:#F8F8FF;
}

#type1{
  background-color: #20B2AA;
}

#type2{
  background-color: #32CD32;
}

#type3{
  background-color: #4169E1;
}

#type4{
  background-color: #191970;
}

#type5{
  background-color: #008080;
}

.arrow{
  background: none;
  width:40rpx;
  height:40rpx;
  background-repeat:no-repeat;
  margin: 50rpx 50rpx 50rpx 30rpx;
}

