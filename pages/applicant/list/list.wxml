<!--应聘者页面-->
<view class="container">
    <view wx:for="{{typeOfJobs}}" wx:for-item="typeOfJob" id="{{typeOfJob.id}}" class="item{{tab[index]}}" data-index="{{index}}" style="right:{{typeOfJob.right}}px"
bindtap="reachToDetails">
        <view class="name">{{typeOfJob.name}}</view>
        <image class="{{isImage[index]}}" src="{{imageSrc}}"></image>
    </view>
</view>

<modal hidden="{{hidden}}" title="出现异常！" confirm-text="确定" bindconfirm="confirm" no-cancel="true">
    出了点小差错！
</modal>